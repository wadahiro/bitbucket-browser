  "license": "MIT",
  "scripts": {
    "postinstall": "typings install",
    "watch": "webpack -w",
    "build:dll": "webpack --config=webpack/webpack.dll.js",
    "build": "useref src/index.html public/index.html --js uglify --css uglify && cross-env NODE_ENV=production webpack -p --config webpack/webpack.config.js",
    "style": "cp -r node_modules/font-awesome/fonts public && node-sass src/css/main.scss public/css/style.css",
    "server": "node-dev test-server.js",
    "start": "cp src/index.html public/index.html && node-dev webpack/webpack.server.js"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-3": "^6.5.0",
    "cross-env": "^1.0.8",
    "express": "^4.13.3",
    "happypack": "^2.1.1",
    "node-dev": "^3.0.0",
    "node-sass": "^3.6.0",
    "react-hot-loader": "^1.3.0",
    "react-transform-hmr": "^1.0.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "ts-loader": "^0.8.2",
    "tslint": "^3.1.1",
    "typescript": "^1.9.0-dev.20160517-1.0",
    "useref-file": "^0.2.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "bulma": "0.0.26",
    "font-awesome": "^4.6.1",
    "lodash": "^4.12.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-select": "^1.0.0-beta13",
    "react-sidebar": "^2.1.2",
    "redux": "^3.5.2",
    "redux-saga": "^0.10.4",
    "whatwg-fetch": "^0.10.1"
  }
}
